@model IEnumerable<TimeTracking.Models.UserTotalHoursViewModel>

@{

    <h2>Total hours per period</h2>
    <h3>Results</h3>
}
<div class="row">
    <div class=" col-md-7">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.TotalHours)
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.TotalHours)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>